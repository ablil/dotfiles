<templateSet group="custom-spring">
  <template name="wire" value="@org.springframework.beans.factory.annotation.Autowired&#10;private " description="Autowire dependency" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="get" value="@GetMapping(&quot;/$PATH&quot;)&#10;ResponseEntity&lt;$TYPE$&gt; $METHOD$() {&#10;&#10;}" description="Get method" toReformat="false" toShortenFQNames="true">
    <variable name="TYPE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="METHOD" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="val" value="@org.springframework.beans.factory.annotation.Value(&quot;${$PROPERTY$}&quot;)&#10;private String $VARIABLENAME$" description="Value from properties file" toReformat="false" toShortenFQNames="true" useStaticImport="true">
    <variable name="PROPERTY" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VARIABLENAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="param" value="@org.springframework.web.bind.annotation.RequestParam(&quot;$PARAM$&quot;) String $PARAM$" description="Request Param" toReformat="false" toShortenFQNames="true" useStaticImport="true">
    <variable name="PARAM" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>