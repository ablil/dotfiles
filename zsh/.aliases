# navigation
alias doc="cd ~/Documents"
alias dl="cd ~/Downloads"
alias dt="cd ~/Desktop"

# ls
alias l="ls -l -a"
alias la="ls -a"
alias ll="ls -l"

# docker
alias dc=docker-compose
alias dps="docker ps"
alias dpsa="docker ps -a"

# git shortcut
alias gcln="git clean -fd"

alias rmr="rm -r"
alias o=xdg-open
alias yt=yt-dlp
alias igrep='grep -i'
alias esed'sed -E'
alias clip="xclip -selection clipboard"
