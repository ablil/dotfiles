[alias]
    s = status
    ss = status -s
    c = commit
    cm = commit -m
    d = diff
[core]
	editor = vi
  excludesfile = ~/.gitignore
  attributesfile = ~/.gitattributes
	autocrlf = input
[status]
	branch = true
[branch]
  # show most recentely changed branches first
  sort = -committerdate
[user]
	email = ablil@protonmail.com
	name = ablil
[init]
	defaultBranch = main
